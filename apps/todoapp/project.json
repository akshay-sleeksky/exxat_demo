{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/todoapp/src",
  "prefix": "ng-mf",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/todoapp",
        "index": "apps/todoapp/src/index.html",
        "main": "apps/todoapp/src/main.ts",
        "polyfills": "apps/todoapp/src/polyfills.ts",
        "tsConfig": "apps/todoapp/tsconfig.app.json",
        "assets": [
          "apps/todoapp/src/favicon.ico",
          "apps/todoapp/src/assets"
        ],
        "styles": [
          "apps/todoapp/src/styles.css"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/todoapp/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/todoapp/src/environments/environment.ts",
              "with": "apps/todoapp/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/todoapp/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-server",
      "configurations": {
        "production": {
          "browserTarget": "todoapp:build:production"
        },
        "development": {
          "browserTarget": "todoapp:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4204,
        "publicHost": "http://localhost:4204"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "todoapp:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/todoapp/**/*.ts",
          "apps/todoapp/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/todoapp"
      ],
      "options": {
        "jestConfig": "apps/todoapp/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "serve-static": {
      "executor": "@nrwl/angular:file-server",
      "defaultConfiguration": "development",
      "options": {
        "port": 4204,
        "buildTarget": "todoapp:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "todoapp:build:development"
        },
        "production": {
          "buildTarget": "todoapp:build:production"
        }
      }
    }
  },
  "tags": []
}